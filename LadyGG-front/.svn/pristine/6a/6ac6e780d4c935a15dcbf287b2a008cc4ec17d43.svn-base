header{position:fixed;display:flex;align-items:center;flex-direction:row-reverse;top:0;right:0;padding:0 20px;width:calc(100% - 250px);height:80px;border-bottom:solid 1px #c9c9c9;background-color:#f5f5f5;z-index:9999}header .admin{padding-right:30px;line-height:80px;text-align:center;font-size:16px}header .admin span{margin-right:10px}header .admin .admin-img{display:inline-block;margin-right:10px;width:20px;height:20px;background:url(../imgae/admin.png) no-repeat 50% 50%;background-size:cover;transform:translateY(2px)}.organ{position:fixed;top:0;left:0;bottom:0;width:250px;text-align:center;background-color:#000}.organ .logo{display:inline-block;width:250px;height:80px;border-bottom:solid 1px #c9c9c9;background:url(../imgae/logo.png) no-repeat 50% 50%;background-size:contain}.organ .uib-title{padding:20px 15px;font-size:20px;color:#fff}.organ .uib-title span{margin-left:-19px}.organ .uib-set{padding:5px 0;background-color:#000;color:#fff}.organ .uib-set a:focus{outline:none;text-decoration:none}.organ .panel-body{padding:0}.organ .uib-get{padding:15px 15px 15px 18px;font-size:14px}.organ .uib-get:hover{background-color:#74d9ff}
