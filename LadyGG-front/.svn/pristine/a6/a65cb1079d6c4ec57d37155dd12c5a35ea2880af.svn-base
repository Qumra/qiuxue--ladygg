.admin-content {
  margin-top: 20px;
  border: solid 1px #cccccc;
}

.admin-content .content-title {
  padding: 0 30px;
  height: 60px;
  border-bottom: solid 1px #cccccc;
  font-size: 20px;
}

.admin-content .content-title p {
  font-weight: bold;
}

.admin-content .admin-table {
  padding: 30px;
}

.admin-content .admin-table .bordered {
  border: 1px solid #ddd;
}

.admin-content .admin-table .bordered .son-border {
  border: 1px solid #ddd;
  vertical-align: middle;
}

.page-box {
  display: flex;
  justify-content: flex-end;
  align-items: center;
  padding: 0 50px;
  height: 60px;
}

.page-box .page-go {
  margin: 0 20px 0 10px;
}

.page-box .page-go .page-input {
  width: 40px;
  text-align: center;
}
