.video-box {
    border: solid 1px #e9e9e9;
    background-color: #f9f9f9;

    .box-top {
        margin-top: 20px;
        padding: 0 50px;
    }

    .box-cen {
        margin-top: 20px;
        padding: 0 50px;
    }

    .box-bottom {
        margin: 20px 0;
        padding: 0 50px;
        text-align: right;
    }

}

.video-content {
    margin-top: 20px;
    border: solid 1px #cccccc;

    .content-title {
        padding: 0 30px;
        height: 40px;
        border-bottom: solid 1px #cccccc;
        font-size: 16px;
    }

    .video-table {
        padding: 15px 30px;
        height: 600px;

        .bordered {
            border: 1px solid #ddd;

            .son-vertical {
                vertical-align: middle;
            }
        }
    }
}

.page-box {
    display: flex;
    justify-content: flex-end;
    align-items: center;
    padding: 0 50px;
    height: 60px;

    .page-go {
        margin: 0 20px 0 10px;

        .page-input {
            width: 40px;
            text-align: center;
        }
    }
}

.tr-back {
    background-color: #666;
}

.classify {
    padding: 0 51px;
}

.grade {
    padding: 0 59px;
}

.subject {
    padding: 0 59px;
}

.videoStatus {
    padding: 0 59px;
}

.praise {
    width: 77.5px;
}

.collection {
    width: 77.5px;
}

.rts {
    width: 250px;

    .rts-btn {
        margin-left: 54px;
    }
}