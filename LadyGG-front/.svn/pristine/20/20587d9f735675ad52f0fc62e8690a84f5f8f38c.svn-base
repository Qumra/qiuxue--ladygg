.PW-box {
    padding-bottom: 100px;
    border: solid 1px #e9e9e9;
    border-radius: 8px;

    .PW-title {
        padding: 0 30px;
        height: 40px;
        background-color: #616663;
        color: #fff;
    }

    .PW-word {
        display: flex;
        flex-direction: column;
        align-items: center;
        padding: 60px 0;

        .PW-input {
            margin: 20px 0;

            .input-titie {
                margin-right: 10px;
                font-size: 14px;
            }

            .input-box {
                width: 300px;
                height: 30px;
            }
        }

        .PW-btn {
            margin-top: 20px;
            width: 380px;
        }
    }

}