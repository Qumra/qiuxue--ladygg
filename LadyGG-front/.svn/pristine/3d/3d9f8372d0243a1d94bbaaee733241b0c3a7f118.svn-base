.ACD-box {
  padding-bottom: 100px;
  border: solid 1px #e9e9e9;
  border-radius: 8px;
}

.ACD-box .ACD-title {
  padding: 0 30px;
  height: 40px;
  background-color: #616663;
  color: #fff;
}

.ACD-box .ACD-word {
  padding: 50px 40px;
}

.ACD-box .ACD-word .ACD-input {
  margin: 10px 0;
  padding: 10px 30px;
}

.ACD-box .ACD-word .ACD-input .input-titie {
  margin-right: 6%;
  width: 56PX;
  font-size: 14px;
  font-weight: 700;
}

.ACD-box .ACD-word .ACD-input .input-box {
  width: 80%;
  height: 30px;
}

.ACD-box .ACD-word .ACD-btn {
  margin: 50px 0;
  padding: 10px 30px 10px 140px;
}
