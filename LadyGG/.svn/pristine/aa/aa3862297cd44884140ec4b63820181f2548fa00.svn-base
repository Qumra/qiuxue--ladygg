<div class=\"message\">\r\n        <img class=\"signSuccess\" src=\"../assets/images/bindSuccess.png\" alt>\r\n        <img class=\"score\" src=\"../assets/images/score.png\" alt>\r\n        <div class=\"bottom-btn\">\r\n          <button class=\"bot-btn\">\r\n            逆袭豆\r\n            <span class=\"addScore\">+{{addScore}}</span>\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { Toast } from \"mint-ui\";\r\nimport Qs from \"qs\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      addScore: 20,\r\n      flag: false,\r\n      msg: \"获取验证码\",\r\n      isDisabled: false,\r\n      count: 60,\r\n      timer: null,\r\n      captcha: \"\",\r\n      email: \"\"\r\n    };\r\n  },\r\n  methods: {\r\n    send() {\r\n      let that = this;\r\n      let id = this.$store.getters.userInfo.userId;\r\n      this.isDisabled = true;\r\n      var data = Qs.stringify({ email: this.email });\r\n      this.axios\r\n        .post(\"/u/user/binding/email/\" + id, data, {\r\n          headers: {\r\n            \"Content-Type\": \"application/x-www-form-urlencoded; charset=UTF-8\"\r\n          }\r\n        })\r\n        .then(response => {\r\n          console.log(response.data);\r\n          if (response.data.code == 0) {\r\n            Toast({\r\n              message: \"验证码已发送\",\r\n              iconClass: \"icon icon-success\"\r\n            });\r\n            that.timer = setInterval(() => {\r\n              that.count--;\r\n              if (that.count > 0) {\r\n                that.msg = \"还剩余\" + that.count + \"秒\";\r\n              } else {\r\n                that.msg = \"重新发送\";\r\n                that.isDisabled = false;\r\n                clearInterval(that.timer);\r\n                that.count = 60;\r\n              }\r\n            }, 1000);\r\n          } else if (response.data.code == 1003) {\r\n            // alert(response.data.message);\r\n            Toast({\r\n              message: \"今天验证已经超过三次了，请明天再来\",\r\n              duration: 3000\r\n            });\r\n          } else {\r\n            // alert(response.data.message);\r\n            Toast({\r\n              message: response.data.message,\r\n              duration: 3000\r\n            });\r\n            that.isDisabled = false;\r\n          }\r\n        })\r\n        .catch(err => {\r\n          console.log(err);\r\n        });\r\n    },\r\n    bind() {\r\n      let that = this;\r\n      let id = this.$store.getters.userInfo.userId;\r\n      let data = Qs.stringify({\r\n        captcha: parseInt(this.captcha),\r\n        email: this.email\r\n      });\r\n      this.axios\r\n        .put(\r\n          `/u/user/binding/email/${id}?email=${this.email}&captcha=${\r\n            this.captcha\r\n          }`\r\n        )\r\n        .then(response => {\r\n          console.log(response.data);\r\n          if (response.data.code == 0) {\r\n            if (response.data.data.increaseScore == 20) {\r\n              // 显示提示框\r\n              that.flag = true;\r\n              setTimeout(() => {\r\n                that.flag = false;\r\n                //  跳转学生卡页面\r\n                that.$router.push({\r\n                  path: \"/card\"\r\n                });\r\n              }, 3000);\r\n            } else {\r\n              Toast({\r\n                message: \"绑定成功\",\r\n                duration: 3000\r\n              });\r\n              setTimeout(() => {\r\n                //  跳转学生卡页面\r\n                that.$router.push({\r\n                  path: \"/card\"\r\n                });\r\n              }, 3000);\r\n            }\r\n          } else {\r\n            Toast({\r\n              message: response.data.message,\r\n              duration: 3000\r\n            });\r\n          }\r\n        })\r\n        .catch(err => {\r\n          console.log(err);\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n@import \"../assets/style/public\";\r\n.disabled-btn {\r\n  ba